            "max_wait_seconds": {
              "type": "integer",
              "description": "当 wait=true 时超时（以秒为单位）。"
            },
            "cwd": {
              "type": "string",
              "description": "命令的绝对工作 目录。"
            }
          },
          "required": [
            "command",
            "wait",
            "max_wait_seconds",
            "cwd"
          ],
          "additionalProperties": false
        }
      }
    },
